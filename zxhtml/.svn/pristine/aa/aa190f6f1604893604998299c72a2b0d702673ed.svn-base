<!-- Mainly scripts -->
<script src="../../js/jquery-2.1.1.js"></script>
<script>
    $(function () {
        var xuanzhe=$('.xuanzhe');
        var almond=$('#ull>li');
        almond.on('click',function () {
            almond.removeClass('active');
            xuanzhe.removeClass('active');
            almond.eq($(this).index()).addClass('active');
            xuanzhe.eq($(this).index()).addClass('active');
        })
        //意见反馈选项卡
        let feed_li=$('.feedback li');
        let feed_ol_li=$('.textarea li ');
       feed_li.on('click',function () {
           feed_li.removeClass('active');
           feed_ol_li.removeClass('active');
           feed_li.eq($(this).index()).addClass('active');
           feed_ol_li.eq($(this).index()).addClass('active');
       })
    })
</script>
