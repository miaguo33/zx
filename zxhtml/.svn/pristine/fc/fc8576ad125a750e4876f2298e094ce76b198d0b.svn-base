.header {
    width: 1210px;
    height: 80px;
    margin: 0 auto;
    background: url(../img/login.png) no-repeat;
    background-position: left center;
    padding-left: 370px;
    box-sizing: border-box; }
.header:after {
    content: "";
    clear: both;
    display: block;
    height: 0;
    line-height: 0; }
.header ul.nav {
    width: 600px;
    height: 100%;
    text-align: justify;
    float: left; }
.header ul.nav:after {
    content: "";
    display: inline-block;
    width: 100%;
    height: 0;
    line-height: 0; }
.header ul.nav li {
    display: inline-block;
    font-size: 16px;
    line-height: 80px;
    position: relative; }
.header ul.nav li.active a {
    color: #cf001c; }
.header ul.nav li.active:after {
    display: block; }
.header ul.nav li:after {
    display: none;
    content: "";
    width: 60px;
    height: 4px;
    background: #cf001c;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto; }
.header ul.nav li a {
    display: inline-block;
    color: #000000;
    transition: all .5s ease;
    padding: 0 15px; }
.header ul.nav li:hover a {
    color: #cf001c; }
.header > a {
    font-size: 16px;
    color: #000000;
    line-height: 80px;
    float: right;
    margin-left: 25px; }
.header > a:hover {
    color: #cf001c; }

