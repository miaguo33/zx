<!--个人注册-->
<div class="register">
    <div class="register_header"><a class="active" href="../register/register.html">个人注册</a><a href="../enterprise/enterprise_reg.html" >企业注册</a></div>
    <form class="form-horizontal">
        <div class="form-group">
            <label for="inputEmail3" class="col-sm-3 control-label" style="width:78px;margin:0;padding:0;letter-spacing:3px;">手机号码:</label>
            <div class="col-sm-9" style="width:350px;margin:0;padding:0;margin-left:15px;">
                <input type="email" class="form-control" id="inputEmail3" placeholder="请输入手机号">
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label" style="width:78px;margin:0;padding:0;">手机验证码:</label>
            <div class="col-sm-9" style="width:350px;margin:0;padding:0;margin-left:15px;display: flex">
                <input type="password" class="form-control col-sm-7" id="" placeholder="请输入手机验证码">
                <!--切换active类名出现60秒-->
                <button class="col-sm-5 span active" id="btn">获取验证码</button>
                <button class="col-sm-4 span" id="btns" style="background: #ccc"><i>60</i>s后重新获取</button>
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label" style="width:78px;margin:0;padding:0;">设置新密码:</label>
            <div class="col-sm-9" style="width:350px;margin:0;padding:0;margin-left:15px;">
                <input type="password" class="form-control" id="inputPassword3" placeholder="请输入新密码">
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label" style="width:78px;margin:0;padding:0;">确认新密码:</label>
            <div class="col-sm-9" style="width:350px;margin:0;padding:0;margin-left:15px;">
                <input type="password" class="form-control" id="inputPassword3" placeholder="请输入新密码">
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label" style="width:78px;margin:0;padding:0;letter-spacing:7px">邀请码:</label>
            <div class="col-sm-9" style="width:350px;margin:0;padding:0;margin-left:15px;">
                <input type="password" class="form-control" id="inputPassword3" placeholder="请输入邀请码">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset- col-sm-10" style="margin:0;padding:0;">
                <div class="checkbox" style="width:350px;margin:0;padding:0;margin-left:93px;">
                    <label>
                        <input type="checkbox" style="margin-top:18px;">我已阅读并同意
                    </label>
                    <a href="">《职业信用管理平台管理服务协议》</a>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-12" style="margin:0;">
                <button type="submit" class="btns">立即注册</button>
            </div>
        </div>
    </form>
</div>
