#free>.ibox-title{
    border-bottom:1px solid #ccc;
    padding-bottom:24px;
}
#free .btn-danger{
    width:80px;
    margin-right:30px;
}
#free .btn-danger:last-of-type{
    margin-right:0;
}

#free>.ibox-content{
    height:auto;
    padding:30px 60px;
    background: #fff;
}
#free>.ibox-content>.content_box{
    width:100%;
    height:100%;
    background: #f6f6f6;
    border-radius: 10px;
    padding:22px;
}
#free>.ibox-content>.content_box>img{
    display: block;
    width:100%;
    height:100%;
}
#brief{
    padding:0;
}
#brief>.function{
    margin-top:36px;
}
#brief>.function>h5{
    font-size:16px;
    color: #989898;
}
#brief>.function>p{
    margin-top:30px;
    font-size:14px;
    color: #000;
}
#brief>.function:nth-of-type(2){
    margin-top:100px;
}