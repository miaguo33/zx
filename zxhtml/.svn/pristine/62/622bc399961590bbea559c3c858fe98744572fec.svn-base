.row{
    margin:0;
    padding:0;
}
.col-lg-12,.col-lg-6{
    margin:0;
    padding:0;
}

.content{
    height:auto;
    background: #fff;
    margin-bottom: 30px;
    padding:60px 100px;
    box-sizing: border-box;
}
.content>.information_header{
    height:177px;
    padding:0;
    box-sizing: border-box;
    padding-bottom: 30px;
}
.content>.information_header>.information_name{
    font-size:24px;
    color: #ddd;
    display: flex;
    justify-content: space-between;
}
.content>.information_header>.information_name>span.active{
    font-weight: bold;
    color: #333;
}
.content>.information_header>.information_Progress{
    height:36px;
    padding:0 32px;
    margin-top: 30px;
    position: relative;
}
.content>.information_header>.information_Progress>.Progress{
    height:10px;
    background: #ddd;
    display: flex;
    justify-content: space-between;
    margin:0 auto;
    border-radius: 5px;
}
.content>.information_header>.information_Progress>.Progress_z{
    height:10px;
    display: flex;
    justify-content: space-between;
    margin-top:-10px;
    padding: 3px;
}
.content>.information_header>.information_Progress>.Progress_z>.Progress_r{
    width: 30px;
    height: 30px;
    box-sizing: border-box;
    border-radius: 50%;
    margin-top: -13px;
    z-index: 100;
    text-align: center;
    line-height: 30px;
    color: #fff;
}
.content>.information_header>.information_Progress>.Progress_z>.Progress_r.active{
    background: #ee2a3a ;
}
.content>.information_header>.information_Progress>.Progress_z>.Progress_r:nth-of-type(2){
    margin-left:-18px;
}
#pro{
    width:10%;
    height:6px;
    background: #ee2a3a ;
    position: absolute;
    top:2px;
    left:38px;
    z-index: 3;
}
.content>.information_header>.information_Progress>.Progress>.Progress_r {
    width: 36px;
    height: 36px;
    background: #ddd;
    box-sizing: border-box;
    border-radius: 50%;
    margin-top: -13px;
    position: relative;
    z-index: 2;
    text-align: center;
    line-height: 30px;
    color: #fff;
    font-size:12px;
    display: flex;
    justify-content: center;
    align-items: center;
    border:3px solid #ddd;
}

.content>.information_header>.information_Progress>.Progress>.Progress_r:nth-of-type(2){
    margin-left:-18px;
}
.content>.information{
    width:100%;
    height:auto;
    margin-top:60px;
}
.content>.information>.edit{
    width:100%;
    display: flex;
    justify-content: space-between;
    padding-bottom: 30px;
    border-bottom: 1px solid #ddd;
}
.content>.information>.edit>span>i{
    font-style: normal;
    color: #ef2a3b;
    font-size:12px;
    margin-left:20px;
}
.content>.information>.edit>span:nth-of-type(1){
    font-size:20px;
    font-weight: bold;
}
.content>.information>.edit>span:nth-of-type(2){
    display: block;
    cursor: pointer;
    width:60px;
    height:30px;
    text-align: center;
    line-height: 30px;
    color: #fff;
    font-size:12px;
    background:#ee2a3a ;
    border-radius: 3px;
}

/*个人信息测试*/
.content>.information>.information_test{
    width:100%;
    height:auto;
}
.content>.information>.information_test>ul{
    width:100%;
    height:100%;
}
.content>.information>.information_test>ul>li{
    margin-top: 30px;
    display: flex;
}
.content>.information>.information_test>ul>li>.li_box{
    width:50%;
    display: flex;
}
.content>.information>.information_test>ul>li>.li_box>span{
    display: block;
}
.content>.information>.information_test>ul>li>.li_box>span:nth-of-type(2){
    margin-left: 10px;
}
.content>.information>.in_personal{
    width:100%;
    height:auto;
    background: #f6f6f6;
    transition: all 0.5s ease;
    border:1px solid #ddd;
    padding:32px;
    display: none;
}
/*选择工作年份日期*/

#jcDate{
    width:100%;
    height:242px;
    border-radius: 6px;
    border: 1px solid #ccc;
}


/*完善个人信息*/
.in_personal>span{
    color: #666;
    font-size:12px;
}

.in_personal>form>.forms{
    display: flex;
    padding:0 70px;
    margin-top:20px
}
.in_personal>form>.forms>span{
    position: absolute;
    bottom:-30px;
    right:70px;
    color: #ccc;
    font-size:12px;
}
.in_personal>form>.forms>span>i{
    font-style: normal;
}
.in_personal>form>.forms.last-child{
    margin-bottom:80px;
}

.in_personal>form>.insert{
    width:100%;
    height:auto;
    margin-top:60px;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.in_personal>form>.insert>span{
    display: block;
    font-size:12px;
    color: #666;
}
.in_personal>form>.insert>i{
    display: block;
    width:auto;
    border:1px dashed #ccc ;
    margin:0 10px;
    flex: 1;
}
.input-group-addon {
    border: 0;
    padding: 0;
    width:0;
}
.in_personal>form>.forms>.form-group>.input-group{
    width:100%;
    height:50px;
    border-radius: 6px;
    margin-top:20px;
}
.in_personal>form>.forms>.form-group>.input-group>input{
    width:100%;
    height:50px;
    border-radius: 6px;
    padding-left:30px;
}
.in_personal>form>.forms:nth-of-type(1){
    margin-top:40px;
}

.in_personal>form>.forms>.form-group:nth-of-type(1){
    margin-right: 48px;
}
.in_personal>form>.forms>.form-group:nth-of-type(2){
    margin-left: 48px;
}
.in_personal>form>.forms>.form-group.text{
    margin-right:0;
    margin-top:40px;
}

.in_personal>form>.forms>.form-group>.form-control{
    height:50px;border:1px solid #ccc;
    border-radius: 6px;
    margin-top:20px;
    background: #fff;
    padding-left:30px;
}
.in_personal>form>.forms>.form-group>.form-control.text{
    height:116px;
    padding:18px 32px;
    resize: none;
}
.in_personal>form>.forms>.form-group>.span{
    height:50px;
    border:1px solid #ccc;
    border-radius: 6px;
    margin-top:20px;
    background: #f6f6f6;
    padding-left:30px;
    line-height: 38px;
}
.in_personal>form>.forms>.form-group.texts{
    margin-right:0;
}
.in_personal>form>.forms>.form-group>.time{
    height:50px;
    margin-top:20px;
    display: flex;
    align-items: center;
}
.in_personal>form>.forms>.form-group>.time>.form-control{
    padding-left:20px;
}
.in_personal>form>.forms>.form-group>.time>span{
    margin:46px;
}
.in_personal>form>.forms>.form-group>label>span{
    display: inline-block;
    color: red;
    margin-left:12px;
}
.in_personal .btnx{
    width:100%;
    height:auto;
    display: flex;
    padding:0 127px;
    justify-content: space-between;
    margin-bottom:48px;
}
.in_personal .btnx>.btnx-box{
    width:250px;
    height:50px;
    border-radius: 6px;
    text-align: center;
    line-height: 50px;
    background: #e70014;
    cursor: pointer;
    color: #fff;
    transition:all 0.3s ease;
}
.in_personal .btnx>.btnx-box:hover{
    background:#c70010 ;
}
.in_personal .btnx>.btnx-box:nth-of-type(1){
    margin-right:130px;
    background: #ccc;
}





/*教育经历*/
.content>.information>.information_test>.education>li{
    display: block;
}
.content>.information>.information_test>.education>li>.time{
    width:100%;
    font-size:14px;
    color: #656565;
    display: flex;
    align-items: center;
}
.content>.information>.information_test>.education>li>.time>.times{
    width:auto;
}
.content>.information>.information_test>.education>li>.time>i{
    display: block;
    width:calc(100% - 95px - 90px);
    border-top:1px dotted #ddd;
    margin:0 5px;
}
.content>.information>.information_test>.education>li>.time>.btn_time{
    width:90px;
    display: flex;
    justify-content: space-between;
}
.content>.information>.information_test>.education>li>.time>.btn_time>span{
    cursor: pointer;
}
.content>.information>.information_test>.education>li .li{
    margin-top: 30px;
    display: flex;
}
.content>.information>.information_test>.education>li .li>.li_box{
    width:50%;
}
.content>.information>.information_test>.education>li .lis{
    color: #000;
    font-weight: bold;
    margin-top:20px;
}
.content>.information>.information_test>.education>li>.Duty{
    width:100%;
    margin-top:30px;
}
.content>.information>.information_test>.education>li>.Duty>.Duty_content{
    margin-top:10px;
}
.content>.information>.information_test>.education>li>.Duty>.Duty_content>ul{
    width:100%;
}
.content>.information>.information_test>.education>li>.Duty>.Duty_content>ul>li{
    margin-top:10px;
}
.content>ul{
    width:100%;
    height:auto;
    margin-top:80px;
    box-sizing: border-box;
    padding:0 68px;
    display: flex;
    justify-content: space-between;
}
.content>ul>li{
    width:200px;
    background: #e70014;
    height:50px;
    border-radius: 8px;
    font-size:16px;
    text-align: center;
    line-height: 50px;
    color: #fff;
    cursor: pointer;
    transition: all 0.3s ease;
    letter-spacing: 2px;
}
.content>ul>li>a{
    display: block;
    width:100%;
    height:100%;
    color: #fff;
}
.content>ul>li:hover{
    background: #c70017;
}
.content>ul>li.preview{
    background: #ccc;
    color: #fff;
}

.content>.no_authentication{
    margin-top:60px;
}
.content>.no_authentication>label{
    display: flex;
}
.content>.no_authentication>label>input{
    width:12px;
    height:12px;
    border:1px solid #ccc;
    margin-right:10px;
}
/*发起个人认证*/
.content>ul.initiate{
    padding:0 150px;
}
.content>ul.Reminders{
    display: flex;
    justify-content: center;
}
.content>ul.Reminders>li{
    width:300px;
}
.content>ul.initiate>li:nth-of-type(1){
    width:50%;
    margin-right:50px;
}
.content>ul.initiate>li:nth-of-type(2){
    width:50%;
    margin-left:50px;
}
#init>.contents{
    display: block;background: rgba(0,0,0,0);border:0;padding:0;
}
#init>.edit{
    border-bottom:0;
    padding-bottom:0;
}
#init>.contents>form>.insert>span{
    font-size:14px;
    color: #000;
}
#init>.contents>form>.insert>span.iconfont{
    color: #e70014;
}
#init>.contents>form>.insert:nth-of-type(1)~.insert{
    margin-top:30px;
}
#init>.contents>form>.insert>.form-group:nth-of-type(1){
    padding-top:10px;box-sizing: border-box
}
#init>.contents>form>.insert>span>.form-control{
    width:320px;height:40px;
    border-radius: 5px;
}
#init>.contents>form>.forms{
    padding:0;
}
#init>.contents>form>.forms>.form-group{
    margin:0;
}
#init>.contents>form>.forms>.form-group>.form-control{
    width:80%;
    height:40px;
    line-height: 40px;
}

/*个人信息确认*/
#init.initiate>.edit{
    border-bottom:1px solid #ccc;
    padding-bottom:30px;
}
#init.initiate>.contents{
    height:auto;
    padding-top:80px;
}
#init.initiate>.contents>.Real_name{
    width:360px;
    height:360px;
    border-radius: 6px;
    border: 1px solid #ccc;
    position: relative;
    box-sizing: border-box;
    padding:70px 50px;
}
#init.initiate>.contents>.Real_name>ul{
    width:100%;
    height:100%;
}
#init.initiate>.contents>.Real_name>ul>li{
    font-size:12px;
    color: #666;
}
#init.initiate>.contents>.Real_name>ul>li:nth-of-type(1)~li{
    margin-top:30px;
}
#init.initiate>.contents>.Real_name>img{
    position: absolute;
    left:0;
    top:0;
    display: block;
    width:100px;
    height:100px;
}
#init.initiate>.contents>.Arrow{
    line-height: 360px;
}

.cs-select{
    padding:0 !important;
}

.form-control.sewv{
    padding:0;
}
.sewvmain{width:1000px;margin:50px auto;text-align: center;}
.sewv{position: relative;width: 100%;height:100%;vertical-align: middle;}
.sewvtop{width:100%;height:100%;cursor:pointer;border-radius: 4px;overflow: hidden;box-sizing: border-box;line-height: 50px;display: flex;align-items: center}
.sewvtop>span{float:left;width:100%;height:100%;white-space:pre;text-overflow:ellipsis;overflow: hidden;font-size: 12px;line-height:50px;color: #666;vertical-align: middle;}
.sewvtop>em{float:right;width: 20px;height: 20px;vertical-align: middle;display: flex;align-items: center}
.sewvtop>em>.img{
    display: inline-block;
    height:0;
    border-top:6px solid rgba(102,102,102,1);
    border-left:5px solid rgba(102,102,102,0);
    border-right:5px solid rgba(102,102,102,0);
}
.sewvbm{width: 100%;position: absolute;left: 0;top: 50px;z-index:200;display: none;border: 1px #D9D9D9 solid;border-radius: 3px;background: #fff}
.sewvbm>li{cursor:pointer;width:100%;height:50px;line-height:50px;font-size: 12px;color: #666;padding-left:30px}
.sewvbm>li:hover{background: #e60012;color: #fff;}


