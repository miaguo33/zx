.red-triangle-frame{
	display: flex;
	flex-flow: row wrap;
}
.red-triangle-frame.item-sm{
	margin: 0 -5px;
}
.red-triangle-frame label{
	margin: 15px;
	flex:auto;
	position: relative;
	background: #f6f6f6;
	border: 1px solid #ededed;
	border-radius: 4px;
	text-align: center;
	cursor: pointer;
	overflow: hidden;
}
.red-triangle-frame label input{
	position:absolute;
	clip: rect(0,0,0,0);
	top: 0;
}

.red-triangle-frame.item-sm label{
	margin: 5px;
	width: auto;
	padding: 10px 20px;
	min-width: 80px;
	font-size: 12px;
}
.red-triangle-frame label:hover{
	border-color: #E60013;
}
.red-triangle-frame label .triangle {
	position: absolute;
	right: 0;
	bottom: -2px;
	width: 0;
	height: 0;
	border-left: 32px solid transparent;
	border-bottom: 32px solid #E60013;
	display: none;
}
.red-triangle-frame.item-sm label .triangle{
	border-left: 16px solid transparent;
	border-bottom: 16px solid #E60013;
}
.red-triangle-frame label .triangle i {
	color: #FFFFFF;
	position: absolute;
	right: 3px;
	top: 11px;
}
.red-triangle-frame.item-sm label .triangle i{
	font-size: 8px;
	right: -1px;
	top: 2px;
}
.red-triangle-frame label.active {
	color: #E60013;
	border: 1px solid #E60013;
	box-shadow: 0 2px 10px 1px #e4e4e4;
}
.red-triangle-frame.item-sm .no-fit{
	width: 50%;
}
.red-triangle-frame label.active .triangle {
	display: block;
}
