.step-arrow li{
	text-align: center;
	color: #FFFFFF;
	white-space: nowrap;
	height: 30px;
	width: 50%;
	display: inline-block;
	position: relative;
}
.step-arrow li h4{
	margin: 0;
	line-height: 30px;
	width: 100%;
}
.step-arrow li:first-child:after{
	position: absolute;
	top: 0;
	right: -18px;
	content: "";
	width: 0;
	height: 0;
	border-top: 15px solid transparent;
	border-bottom: 15px solid transparent;
	border-left: 18px solid #E60013;
}
.step-arrow li.active{
