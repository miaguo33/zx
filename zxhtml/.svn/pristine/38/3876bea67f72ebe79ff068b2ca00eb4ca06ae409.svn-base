.content>.information>.edit{
    border-bottom:0;
}
.content>.information>.edit>span:nth-of-type(1){
    display: flex;
    align-items: center;
    margin-left:-20px;
}
.information>.edit>span>i{
    display: block;
    width:4px;
    height:14px;
    background: #e60012;
    margin-right:10px;
}
#card{
    border:0;
    background:#fff;
}
.content>ul{
    justify-content: center;
}
#card>form>.forms>.form-group{
    position: relative;
}
#card>form>.forms>.form-group>.portrait{
    width:140px;
    height:140px;
    border-radius: 50%;
    background: #ccc;
    position: absolute;
    left:0;right:0;margin:auto;
    background-size:cover ;
    background-position: center center;
}
#card>form>.forms>.form-group>.label{
    width:140px;
    height:30px;
    text-align: center;
    line-height: 24px;
    border-radius: 5px;
    background: #e60012;
    color: #fff;
    position: absolute;
    left:0;right:0;margin:auto;
    top:155px;
    z-index: 200;
    font-size:14px;
}
.information .contents>.list{
    height:570px;
    box-sizing: border-box;
    padding-right:5px;

}
.information .contents>.list>ul{
    border:1px solid #ccc;
    width:408px;
    height:100%;
    overflow-y: scroll;
    padding:32px 22px;
    border-radius: 6px;
}
::-webkit-scrollbar-track-piece{
    background: #f6f6f6;
    -webkit-border-radius: 6px;
}
::-webkit-scrollbar{
    width:10px;
    height:20px;
}
.information .contents>.list>ul>li{
    float: left;
    width:104px;
    height:156px;
    margin-right:20px;
    cursor: pointer;
}
.information .contents>.list>ul>li:nth-of-type(3)~li{
    margin-top:20px;
}
.information .contents>.list>ul>li>img{
    display: block;
    width:100%;
    height:100%;
}
.information .contents>.list>ul>li:nth-of-type(3n+0){
    margin-right:0;
}
.information .contents>.show{
    height:570px;
    display: flex;
}
.information .contents>.show>.phone{
    width:276px;
    height:100%;
    background: url("../img/lbs-shouji.png") no-repeat;
    background-position: center center;
    background-size:cover;
    margin:0 auto;
    display: flex;
    justify-content: center;
    align-items: center;
}
.information .contents>.show>.phone>.img{
    width:240px;
    height:433px;
}
.information .contents>.show>.phone>.img>.card_one{
    width:100%;
    height:100%;
    position: relative;
    border-top:1px solid rgba(0,0,0,0);
}
.information .contents>.show>.phone>.img>.card_one>img{
    display: block;
    width:100%;
    height:100%;
    position: absolute;
    left:0;
    top:2px;
}
.information .contents>.show>.phone>.img>.card_one>.portrait{
    width:80px;
    height:80px;
    border-radius: 50%;
    position: absolute;
    background: #9d8e8f;
    top:58px;
    left:50px;
    background-position: center center;
    background-size:cover;
}
.information .contents>.show>.phone>.img>.card_one>.portrait>img{
    display: block;
    width:100%;
    height:100%;
}
.information .contents>.show>.phone>.img>.card_one>.top{
    margin-top:81px;
    position: relative;
    z-index: 10;
    display: flex;
    justify-content: flex-end;
}
.information .contents>.show>.phone>.img>.card_one>.top>.right{
    display: flex;
    align-items: center;
    padding-right: 10px;
}
.information .contents>.show>.phone>.img>.card_one>.top>.right>h3{
    font-size:14px;
    margin-right:6px;
}
.information .contents>.show>.phone>.img>.card_one>.top>.right>p{
    font-size:12px;
    color: #6e6c6d;
}
.information .contents>.show>.phone>.img>.card_one>p{
    font-size:12px;
    text-align: center;
    margin-top:28px;
    position: relative;
    z-index: 10;
}
.information .contents>.show>.phone>.img>.card_one>ul{
    width:100%;
    height:auto;
    position: relative;
    z-index: 10;
    padding-left:40px;
    margin-top:15px;
}
.information .contents>.show>.phone>.img>.card_one>ul>li{
    font-size:12px;
    color: #40494b;
    margin-top:5px;
    display: flex;
}
.information .contents>.show>.phone>.img>.card_one>ul>li>i{
    margin-right:5px;
    color: #3da87e;
}
.information .contents>.show>.phone>.img>.card_one>.erweima{
    position: relative;
    z-index: 20;
    text-align: center;
    height:auto;
}
.information .contents>.show>.phone>.img>.card_one>.erweima>img{
    display: block;
    width:53px;
    height:53px;
    margin:0 auto;
}
.information .contents>.show>.phone>.img>.card_one>.erweima>p{
    font-size:12px;
}
.information{
    text-align: center;
}
.information>p{
    margin-top:132px;
    font-size:12px;
    color: #000;
    font-weight: bold;
}
.information>img{
    display: block;
    margin:50px auto;
}