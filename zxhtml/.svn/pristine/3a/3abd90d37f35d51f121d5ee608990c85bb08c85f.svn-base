<script>
    $(function () {
        //选项卡切换
        let xuanzhe=$('.xuanzhe');
        let almond=$('#ull>li');
        almond.on('click',function () {
            almond.removeClass('active');
            xuanzhe.removeClass('active');
            almond.eq($(this).index()).addClass('active');
            xuanzhe.eq($(this).index()).addClass('active');
        })
        //验证码时间
        let times=$('#times');
        let timess=$('#timess');
        let index=5;
        times.on('click',function () {
            $(this).removeClass('active');
            timess.addClass('active').css('background','rgba(0,0,0,0.2)');
        })


    })
</script>
