<script>

    // 是否在数组内
    function in_array(needle, haystack) {
        if (typeof needle == 'string' || typeof needle == 'number') {
            for (var i in haystack) {
                if (haystack[i] == needle) {
                    return true;
                }
            }
        }
        return false;
    }
</script>
<script>
    $(function () {

        //点击编辑或者添加所触发的效果
        let information = $('.information');
        information.on('click', '.btns', function () {
            //获取当前元素里的展示列表
            let infromation_test = $(this).parents('.information').find('.information_test');
            //获取当前元素里的编辑列表
            let contents = $(this).parents('.information').find('.contents');
            $(this).css('display', 'none');
            infromation_test.css('display', 'none');
            contents.css('display', 'block');
        })
        //点击编辑里保存所触发的效果
        let contents = $('.information>.contents');
        //点击编辑里确认的时候触发的事件
        contents.on('click', '.btnx>.yes', function () {
            //获取当前编辑列表元素
            let contents = $(this).parents('.contents');
            //获取当前父级元素的展示列表
            let information = $(this).parents('.information').find('.information_test');
            let btns = $(this).parents('.information').find('.btns');
            btns.css('display', 'block');
            contents.css('display', 'none');
            information.css('display', 'block');
        })
        //点击编辑里取消的时候触发的事件
        contents.on('click', '.btnx>.no', function () {
            //获取当前编辑列表元素
            let contents = $(this).parents('.contents');
            //获取当前父级元素的展示列表
            let information = $(this).parents('.information').find('.information_test');
            let btns = $(this).parents('.information').find('.btns');
            btns.css('display', 'block');
            contents.css('display', 'none');
            information.css('display', 'block');
        })
    })
