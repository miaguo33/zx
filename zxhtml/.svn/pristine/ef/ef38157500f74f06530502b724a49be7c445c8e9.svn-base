#recharge .modal-body {
	padding: 20px 2% 40px;
}

#recharge ul {
	width: auto;
	margin: 0 auto;
	overflow: hidden;
}

#recharge li {
	float: left;
	position: relative;
	width: 27%;
	height: 100px;
	margin: 10px 2.5%;
	background: #f6f6f6;
	border: 1px solid #ededed;
	border-radius: 4px;
	text-align: center;
	cursor: pointer;
}

#recharge li h2 {
	color: #000000;
	font-weight: 600;
	margin: 26px 0 8px;
}

#recharge li h5 {
	color: #999999;
}

#recharge li div {
	position: absolute;
	right: 0;
	bottom: 0;
	width: 0;
	height: 0;
	border-left: 32px solid transparent;
	border-bottom: 32px solid #E60013;
	display: none;
}

#recharge li div i {
	color: #FFFFFF;
	position: absolute;
	right: 3px;
	top: 11px;
}

#recharge li.active {
	border: 1px solid #E60013;
}

#recharge li.active div {
	display: block;
}

