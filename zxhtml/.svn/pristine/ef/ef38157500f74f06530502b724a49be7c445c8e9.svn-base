body {
	font-size: 14px;
	color: #333333;
}

ul,
li {
	margin: 0;
	padding: 0;
	list-style: none;
}
.gray-bg{
	background: #f6f6f6;
}
.text-success {
	color: #1c94d2;
}

.wrapper-content {
	padding: 30px 10px 0;
}

.input-group-addon {
	border: 0;
	padding: 0;
}

.btn {
	letter-spacing: 1px;
	height: 30px;
	padding: 1px 16px;
	border-radius: 6px;
}

.btn-sm {
	height: 40px;
	min-width: 100px;
	padding: 1px 16px;
	border-radius: 4px;
}

.btn-outline {
	font-size: 14px;
	height: 30px;
	min-width: 80px;
	padding: 1px 16px;
	border-radius: 4px;
}

.btn-outline.btn-default {
	border: 1px solid #CCCCCC;
	color: #999999;
}

.btn-outline.btn-default:hover,
.btn-outline.btn-default:focus,
.btn-outline.btn-default:active,
.btn-outline.btn-default.active,
.btn-outline.open .dropdown-toggle.btn-default,
.btn-outline.btn-default:active:focus,
.btn-outline.btn-default:active:hover,
.btn-outline.btn-default.active:hover,
.btn-outline.btn-default.active:focus {
	background: transparent;
	color: #999999;
}

.btn-outline.btn-danger:hover,
.btn-outline.btn-danger:focus,
.btn-outline.btn-danger:active,
.btn-outline.btn-danger.active,
.btn-outline.open .dropdown-toggle.btn-danger,
.btn-outline.btn-danger:active:focus,
.btn-outline.btn-danger:active:hover,
.btn-outline.btn-danger.active:hover,
.btn-outline.btn-danger.active:focus {
	background: #E60013;
}


