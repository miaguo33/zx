div.cs-select{
	max-width: auto;
}
.cs-select > span::after{
	font-size: 30px;
    color: #666666;
    right: 20px !important;
    top: 46% !important;
}
.cs-placeholder{
	color: #cccccc;
}
.cs-select{
	padding: 0;
}
.cs-select span{
	width: 100%;
	height: 100%;
}
.cs-select .cs-options{
	margin-top: 10px;
	border: 1px solid #ccc;
	border-radius: 5px;
}
.cs-select .cs-selected{
	background: #E60013;
	color: #FFFFFF;
}
.cs-select .cs-selected span::after{
	display: none;
}
.cs-select.cs-active > span::after{
	transform: translateY(-50%) rotate(0deg) !important;
}
.cs-placeholderselected{
	color: #333333;
}
.cs-select.cs-active .cs-placeholderselected{
	color: #cccccc;
}
.cs-select select{
	visibility: hidden;	
}
