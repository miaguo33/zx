.modal-content {
	overflow: hidden;
}

.modal-header {
	background-color: #2e2d2d;
	border-bottom: 2px solid #e60013;
	color: #FFFFFF;
	padding: 24px 30px;
}

.modal-header .close {
	opacity: 1;
	color: #FFFFFF;
	text-shadow: none;
}

.modal-body {
	padding: 50px 30px 0px;
}

.form-horizontal .control-label {
	padding: 0 0 0 15px;
	line-height: 50px;
	text-align: left;
}

.modal-body .control-label.text-only {
	line-height: 20px;
	text-align: left;
}

.modal-body .control-label.text-only span {
	width: auto;
}

.control-label span {
	font-weight: 100;
}
.control-label span:first-child{
	display: inline-block;
	width: 68px;
	text-align-last: justify;
}
.modal-body .form-group {
	margin-bottom: 30px;
}

.form-control {
	outline: none;
	border: 1px solid #ccc;
	height: 50px;
	border-radius: 5px !important;
	box-shadow: 0 0 10px 1px rgba(0, 0, 0, 0.1);
	padding-left: 30px;
}
.form-control:focus{
	box-shadow: 0 0 10px 1px rgba(230, 0, 19, 0.2);
}
.input-group .form-control:not(:first-child):not(:last-child),
.input-group-addon:not(:first-child):not(:last-child),
.input-group-btn:not(:first-child):not(:last-child) {
	border-radius: 5px;
}

.modal-body .text-danger.price {
	font-size: 18px;
	padding-left: 22px;
	line-height: 50px;
}

